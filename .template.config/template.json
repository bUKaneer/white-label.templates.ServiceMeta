{
  "$schema": "http://json.schemastore.org/template",
  "author": "Luke Baughan @bUKaneer",
  "classifications": ["Web", "ASP.NET", "Clean Architecture"],
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "identity": "WhiteLabel.CleanArchitecture.ServiceMeta.Template",
  "name": "WhiteLabel Clean Architecture Service Meta Template",
  "shortName": "whitelabel-service",
  "sourceName": "WhiteLabel.ServiceMeta",
  "preferNameDirectory": true,
  "symbols": {
    "UserInterfaceHttpPort": {
      "type": "generated",
      "generator": "port",
      "parameters": {
        "low": 10000,
        "high": 50000
      },
      "replaces": "USERINTERFACE_PORT"
    },
    "UserInterfaceHttpsPort": {
      "type": "generated",
      "generator": "port",
      "parameters": {
        "low": 10000,
        "high": 50000
      },
      "replaces": "USERINTERFACE_HTTPS_PORT"
    },
    "ApiHttpPort": {
      "type": "generated",
      "generator": "port",
      "parameters": {
        "low": 10000,
        "high": 50000
      },
      "replaces": "API_PORT"
    },
    "ApiHttpsPort": {
      "type": "generated",
      "generator": "port",
      "parameters": {
        "low": 10000,
        "high": 50000
      },
      "replaces": "API_HTTPS_PORT"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/[Bb]in/**",
            "**/[Oo]bj/**",
            "**/.template.config",
            "**/*.filelist",
            "**/*.user",
            "**/*.lock.json",
            "**/.git/**",
            "**/.vs/**",
            "**/.vscode/**",
            "**/.idea/**",
            "**/.nuspec"
          ]
        }
      ]
    }
  ]
}
